{
  "name": "hcm.fab.myleaverequest.hcmfab_leav_manextension",
  "version": "0.0.1",
  "description": "",
  "devDependencies": {
    "@sap/ui5-builder-webide-extension": "1.0.x",
    "@ui5/cli": "^3.0.0",
    "@sap/ux-ui5-tooling": "1",
    "rimraf": "3.0.2"
  },
  "scripts": {
    "build_old": "ui5 build --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
    "start": "fiori run --open \"index.html?sap-client=602&sap-ui-xx-viewCache=false#title-display\"",
    "build": "ui5 build -a --clean-dest --include-task=generateCachebusterInfo",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml",
    "deploy-config": "fiori add deploy-config",
    "start-ui5-flp": "fiori run --open \"/test-resources/sap/ushell/shells/sandbox/fioriSandbox.html?sap-theme=sap_bluecrystal&sap-client=602&sap-ushell-test-url-url=..%2F..%2F..%2F..%2F..%2F..&sap-ushell-test-url-additionalInformation=SAPUI5.Component%3Dhcm.fab.myleaverequest.HCMFAB_LEAV_MANExtension#Test-url\"",
    "undeploy": "npm run build && fiori undeploy --config ui5-deploy.yaml",
    "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
  },
  "ui5": {
    "dependencies": [
      "@sap/ui5-builder-webide-extension"
    ]
  },
  "private": true,
  "sapuxLayer": "CUSTOMER_BASE",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html"
}
